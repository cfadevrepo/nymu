{
  "name": "taipei",
  "version": "0.1.0",
  "description": "Chinese Overseas Flagship Program",
  "author": "Neetos LLC",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/neetosllc/taipei",
  "main": "dist/server.js",
  "scripts": {
    "watch:server": "webpack --mode development --watch --watch-aggregate-timeout 1000 --display-error-details true --config webpack.server.config.js",
    "watch:client": "webpack-dev-server --mode development --hot --inline --config webpack.client.config.js",
    "watch:node": "wait-on file:./dist/server/index.js && PORT=4000 ASSET_BASE_PATH=http://localhost:4001 nodemon -r source-map-support/register --verbose --watch ./dist/server ./dist/server/index.js",
    "start:dev": "concurrently -k -n \"API:BUILD,API:SERVER,CLIENT\" -c \"red,yellow,bgMagenta\" \"npm run watch:server\" \"npm run watch:node\" \"npm run watch:client\"",
    "build:server": "webpack --mode production --progress --config webpack.server.config.js",
    "build:client": "webpack --mode production --progress --config webpack.client.config.prod.js",
    "build": "npm run build:server && npm run build:client",
    "start": "NODE_ENV=production node ./dist/server/index.js",
    "lint-report": "eslint -o checkstyle-result.xml -f checkstyle --fix --ext .js --ext .jsx .",
    "lint-fix": "eslint --fix --ext .js --ext .jsx ."
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "helmet": "^3.21.2",
    "jss": "^9.8.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-jss": "^8.6.1",
    "react-lazyload": "^2.6.1",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "reactjs-popup": "^1.3.2",
    "redux": "^4.0.1",
    "styled-components": "^4.2.0",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-transform-template-literals": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.6.5",
    "react-player": "^2.6.0",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
